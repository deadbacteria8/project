<%- include('header.ejs') %>
<div class="maindiv">
<form method="post" enctype="multipart/form-data">
    <label for="csvFile">Choose a CSV file:</label>
    <input type="file" name="csvFile" id="csvFile" accept=".csv">
    <input type="submit" value="Upload">
</form>
<% if (csvHandleOutput !== undefined) { %>
    <p><%= csvHandleOutput %></p>
<% } %>
</div>
<%- include('footer.ejs') %>